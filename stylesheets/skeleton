        .container .thirteen.columns                { width: 604px; }
        .container .fourteen.columns                { width: 652px; }
        .container .fifteen.columns                 { width: 700px; }
		
/*EDIT*/.container .full.columns                 { width: 96%; }
/* 96% + 2% padding on each side of container column */
		
		/*  padding-left + padding-right + padding-left + padding-right */
		/*  1st col  2%	 + 1st col  2%	 + 2nd col 2%	+ 2nd col 2% = 8%	*/
		/* 	92% free 	= 	35% + 57% 			(35 x 1.618 = 56.631)  */
		.container .first.column 	/*EDIT*/		{ width: 35%; position: relative; top:5%}
		.container .second.column 	/*EDIT*/		{ width: 57%; float:right;}
		.container .last.column 	/*EDIT*/		{ width: 35%; position: relative; top:5%}

        /* Offsets */
        .container .offset-by-one                   { padding-left: 9%; }
        .container .offset-by-two                   { padding-left: 17%; }
        .container .offset-by-three                 { padding-left: 25%; }
        .container .offset-by-four   /*ALL*/        { padding-left: 33%; }
        .container .offset-by-five   /*EDITED*/     { padding-left: 41%; }
        .container .offset-by-six                   { padding-left: 49%; }
        .container .offset-by-seven                 { padding-left: 57%; }
        .container .offset-by-eight                 { padding-left: 65%; }
        .container .offset-by-nine                  { padding-left: 73%; }
        .container .offset-by-ten                   { padding-left: 81%; }
        .container .offset-by-eleven                { padding-left: 89%; }
        .container .offset-by-twelve                { padding-left: 97%; }
    }


